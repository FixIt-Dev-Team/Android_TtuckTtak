<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/onboarding_">

    <ImageView
        android:id="@+id/iconBackButton"
        android:layout_width="11.4dp"
        android:layout_height="18.72dp"
        android:src="@drawable/ic_back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.067" />

    <TextView
        android:id="@+id/profileSettingsTextContext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="프로필 설정"
        android:textColor="#000000"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.355"
        app:layout_constraintStart_toEndOf="@+id/iconBackButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.061" />

    <ImageView
        android:id="@+id/checkImage"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:src="@drawable/check_image"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toEndOf="@+id/profileSettingsTextContext"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.061" />

    <ImageView
        android:id="@+id/profileImage"
        android:layout_width="166.5dp"
        android:layout_height="166dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:src="@drawable/profile_image"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profileSettingsTextContext"
        app:layout_constraintVertical_bias="0.069" />

    <ImageView
        android:id="@+id/proflieImagePlusImage"
        android:layout_width="47dp"
        android:layout_height="47dp"
        android:src="@drawable/profile_image_plus"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.67"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.284" />

    <RelativeLayout
        android:id="@+id/textboxNikname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/white_box"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profileImage"
        app:layout_constraintVertical_bias="0.072">

        <ImageView
            android:id="@+id/goNiknameButtons"
            android:layout_width="7.62dp"
            android:layout_height="12.79dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="303dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:src="@drawable/ic_front" />

        <TextView
            android:id="@+id/niknameText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="15dp"
            android:gravity="top"
            android:text="닉네임"
            android:textColor="#000000"
            android:textSize="14sp" />

        <View
            android:id="@+id/rectangle1"
            android:layout_width="1dp"
            android:layout_height="18dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="70dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="17dp"
            android:background="#1DB9DB" />

        <TextView
            android:id="@+id/niknameTtukttak"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="85dp"
            android:gravity="top"
            android:text="김뚝딱이"
            android:textColor="#000000"
            android:textSize="14sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/textboxEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:background="@drawable/white_box"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textboxNikname"
        app:layout_constraintVertical_bias="0.049">

        <TextView
            android:id="@+id/emailText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="15dp"
            android:gravity="top"
            android:text="이메일"
            android:textColor="#000000"
            android:textSize="14sp" />

        <View
            android:id="@+id/rectangle_4"
            android:layout_width="1dp"
            android:layout_height="18dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="70dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="17dp"
            android:background="#1DB9DB" />

        <TextView
            android:id="@+id/emailContext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginStart="85dp"
            android:layout_marginTop="15dp"
            android:gravity="top"
            android:text="ttukttak@ttukttak.com"
            android:textColor="#000000"
            android:textSize="14sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/passwordTextbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="127dp"
        android:background="@drawable/green_box"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textboxEmail"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/passwordButtonsText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="99dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="99dp"
            android:gravity="top"
            android:text="비밀번호 재설정하기"
            android:textColor="#FFFFFF"
            android:textSize="14sp" />

        <ImageView
            android:id="@+id/goPasswordButtons"
            android:layout_width="7.62dp"
            android:layout_height="12.79dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="303dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:src="@drawable/ic_front_white" />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>